            <AnchorPane fx:id="container_img" maxWidth="250.0" minWidth="250.0" prefWidth="250.0">
               <children>
                  <ImageView fx:id="node_img" fitWidth="250.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@image-edit.png" />
                     </image>
                  </ImageView>
               </children>
            </AnchorPane>
            <TextField fx:id="node_imgURL" promptText="[img_url]" style="-fx-border-color: black; -fx-border-width: 1;" />
            <TextField fx:id="node_name" promptText="[name]" style="-fx-border-color: black; -fx-border-width: 1;" />
            <Button fx:id="node_update" alignment="CENTER" maxWidth="1.7976931348623157E308" mnemonicParsing="false" style="-fx-background-color: black; -fx-cursor: hand;" text="Actualizar" textFill="WHITE" />
         </children>
      </VBox>
      <Button fx:id="node_navigation" mnemonicParsing="false" style="-fx-background-color: black; -fx-cursor: hand;" text="   â¦¿   " textFill="WHITE">
         <font>
